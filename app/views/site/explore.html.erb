<% meta_keywords "Joyviewer, Joyviewer.com, instagram,instagram feed, instagram login, IG, web, PC, Computer, viewer, instagram on web" %>
<% meta_description "Joyviewer - Joy of Instagram On Web" %>
<h1><%=h content_for(:title, "Joyviewer - Joy of Instagram On Web @Joyviewer") %></h1>
<%= render :partial => "site/aol_leaderboard", :locals => {:show => @interesting_user_posts.present?} %>
<div class="head-text">Explore Instagrammers</div>
<div class="row">
  <% @interesting_user_posts.each do |post| %>
	  <div class="col-xs-6 col-md-3">
	    <a href="/media/<%= post.media_id %>" class="thumbnail">
	      <img src="<%= post.image_low %>" alt="...">
	    </a>
	  </div>
    <% end %>   
</div>
