<div class="popular-text">Explore Instagram Users</div>
  <div class="grid_12">
    <% @interesting_user_posts.each do |user_id,user_posts| %>
      <div class="explore">
        <div align="center" style="margin: 50px 0px;">
          <a href="/u/<%= @interesting_users[user_id].instagram_username %>" style="color: #202020;"><img src="<%= @interesting_users[user_id].instagram_profile_picture %>" style="border: 1px solid #202020;"><br>
          <%= @interesting_users[user_id].instagram_username %></a>
        </div>
        <div>  
          <% user_posts[0..8].each do |post| %>
            <a href="/media/<%= post.media_id %>"><img src="<%= post.image_low %>" style="border: 2px solid #202020;margin:-2px;border-radius:0px;"></a>
          <% end %>   
      </div> 
    </div>
    <% end %>