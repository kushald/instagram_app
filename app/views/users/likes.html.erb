<% if params[:action] == "geo_tag_content" %>
  <div class="popular-text"> Instagram Pictures near & around you</div>
  <%= render :partial => "site/leaderboard_ad" %>
<% else %>
  <%= render '/users/user_basic_info' %>
<% end %>
<%#= render '/site/content' %>
<div class="row">
  <% @data["data"].each_with_index do |data,i| %>
    <div class="large-3 columns" style="padding:5px;">
      <% if data["type"] == "video" %>
        <a href="/media/<%= data["id"] %>"><div class="arrow-right"></div></a>
      <% end %>
      <a href="/media/<%= data["id"] %>"><img src="<%= data["images"]["low_resolution"]["url"] %>" style="border: 1px solid #202020;margin:-2px;border-radius:0px;"></a>
    </div>
    <% end %>
</div>
<%= render "/users/pagination" %>
<%= render :partial => "site/leaderboard_ad" %>