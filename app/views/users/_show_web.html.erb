<% if @data.blank? %>
  <%= render :partial => "site/leaderboard_ad", :locals => {:keyword => params[:id]} %>
  <h3 class="error">User does not exist or user deleted profile</h3>
<% elsif @temp_info.blank? %>
  <%= render :partial => "site/leaderboard_ad", :locals => {:keyword => params[:id]} %>
  <h3 class="error">Sorry this is a private User</h3>
<% else %>
  <%= render :partial => "site/aol_leaderboard", :locals => {:show => @data["data"].present?} %>

  <%#= render :partial => "site/leaderboard_ad", :locals => {:keyword => @temp_info["user"]["username"]} %>
  <div class="row">
    <div class="large-6 columns">
      <div class="large-3 columns">Share this page:</div>
      <div class="small-1 columns">
        <div id="fb-share" link="<%= request.protocol+request.host_with_port+request.fullpath %>" picture="<%= @temp_info["user"]["profile_picture"] %>" caption="<%= request.protocol+request.host_with_port+request.fullpath+ request.path_info %>">
          <a><i class="social foundicon-facebook"></i></a>
        </div>
      </div>
      <div class="small-2 columns">
        <a href="https://twitter.com/share" class="twitter-share-button" data-via="joyviewer" data-related="joyviewer" data-hashtags="joyviewer">Tweet</a>
        <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
      </div>
      <div class="small-2 columns">
        <a href="http://www.tumblr.com/share" title="Share on Tumblr" style="display:inline-block; text-indent:-9999px; overflow:hidden; width:61px; height:20px; background:url('http://platform.tumblr.com/v1/share_2.png') top left no-repeat transparent;">Share on Tumblr</a>
      </div>
      <div class="small-3 columns"></div>
    </div>
  </div>
  <%= render :partial => 'user_basic_info', :locals => {:show_follow => true} %>
  <div class="row">
    <div class="large-8 columns large-centered" style="margin-bottom: 4%;">
      <% 3.times do %>
        <SCRIPT>var ACE_AR = {Site: '885816', Size: '180150'};</SCRIPT> <SCRIPT language='JavaScript' src='http://uac.advertising.com/wrapper/aceUAC.js'></SCRIPT>
      <% end %>
    </div>
  </div>
  <%#= render :partial => "site/leaderboard_ad", :locals => {:keyword => @temp_info["user"]["username"]} %>
  <%#= render 'uploads' %> 
  <%= render '/site/content' %>
<% end %>